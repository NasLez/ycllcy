{"ast":null,"code":"import axios from \"axios\";\nimport store from \"@/vuex/store\";\nexport default {\n  name: \"UpdateChannel\",\n  store,\n  data() {\n    return {\n      userinfo: {\n        id: '',\n        name: '',\n        type: '',\n        creator: '',\n        creatorEmail: '',\n        score: '',\n        due: ''\n      },\n      activeName: '',\n      labelPosition: 'right',\n      updateChannel: {\n        name: '',\n        type: '',\n        creator: '',\n        creatorEmail: '',\n        score: '',\n        due: ''\n      },\n      options: [{\n        value: 'Thesis',\n        label: '论文'\n      }, {\n        value: 'Project',\n        label: '项目'\n      }],\n      value: ''\n    };\n  },\n  created: function () {\n    this.channelid = this.$store.state.channel.id;\n    axios.get(`mu/getChannelById?id=${this.channelid}`).then(res => {\n      this.userinfo.id = res.data.id;\n      this.userinfo.name = res.data.name;\n      if (res.data.type === \"Thesis\") {\n        this.userinfo.type = \"论文\";\n      } else if (res.data.type === \"Project\") {\n        this.userinfo.type = \"项目\";\n      }\n      this.userinfo.creator = res.data.creator;\n      this.userinfo.creatorEmail = res.data.creatorEmail;\n      this.userinfo.score = res.data.score;\n      this.userinfo.due = res.data.due;\n    });\n  },\n  methods: {\n    parseTime(time, cFormat) {\n      if (arguments.length === 0) {\n        return null;\n      }\n      const format = cFormat || '{y}-{m}-{d} {h}:{i}:{s}';\n      let date;\n      if (typeof time === 'object') {\n        date = time;\n      } else {\n        if (typeof time === 'string' && /^[0-9]+$/.test(time)) {\n          time = parseInt(time);\n        }\n        if (typeof time === 'number' && time.toString().length === 10) {\n          time = time * 1000;\n        }\n        date = new Date(time);\n      }\n      const formatObj = {\n        y: date.getFullYear(),\n        m: date.getMonth() + 1,\n        d: date.getDate(),\n        h: date.getHours(),\n        i: date.getMinutes(),\n        s: date.getSeconds(),\n        a: date.getDay()\n      };\n      const time_str = format.replace(/{([ymdhisa])+}/g, (result, key) => {\n        const value = formatObj[key];\n        // Note: getDay() returns 0 on Sunday\n        if (key === 'a') {\n          return ['日', '一', '二', '三', '四', '五', '六'][value];\n        }\n        return value.toString().padStart(2, '0');\n      });\n      return time_str;\n    },\n    handleClick(tab, event) {\n      console.log(tab, event);\n    },\n    updateChannelNow() {\n      this.look = this.$store.state.channel.id;\n      this.due = this.$data.updateChannel.due.toString();\n      axios.post(`mu/updateChannel?id=${this.look}&name=${this.userinfo.name}&type=${this.userinfo.type}&creator=${this.userinfo.creator}&creatorEmail=${this.userinfo.creatorEmail}&score=${this.userinfo.score}&due=${this.$data.updateChannel.due}`).then(res => {\n        if (res.status === 400) {\n          this.$message({\n            message: \"serve error\",\n            type: 'warning'\n          });\n        } else if (res.status === 403) {\n          this.$message({\n            message: \"Unauthorized\",\n            type: 'warning'\n          });\n        } else if (res.status === 404) {\n          this.$message({\n            message: \"Name Not Found\",\n            type: 'warning'\n          });\n        } else if (res.status === 200) {\n          this.$message({\n            message: res.data,\n            type: 'success'\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA8DA;AACA;AAEA;EACAA;EACAC;EACAC;IACA;MACAC;QACAC;QACAJ;QACAK;QACAC;QACAC;QACAC;QACAC;MACA;MACAC;MACAC;MACAC;QACAZ;QACAK;QACAC;QACAC;QACAC;QACAC;MACA;MACAI;QACAC;QACAC;MACA;QACAD;QACAC;MACA;MACAD;IACA;EACA;EACAE;IACA;IACAC;MACA;MACA;MACA;QACA;MACA;QACA;MACA;MACA;MACA;MACA;MACA;IACA;EACA;EACAC;IACAC;MACA;QACA;MACA;MACA;MACA;MACA;QACAC;MACA;QACA;UACAC;QACA;QACA;UACAA;QACA;QACAD;MACA;MACA;QACAE;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA;MACA;QACA;QACA;QACA;UAAA;QAAA;QACA;MACA;MACA;IACA;IACAC;MACAC;IACA;IACAC;MACA;MACA;MACAd;QACA;UACA;YACAe;YACA3B;UACA;QACA;UACA;YACA2B;YACA3B;UACA;QACA;UACA;YACA2B;YACA3B;UACA;QACA;UACA;YACA2B;YACA3B;UACA;QACA;MACA;IACA;EACA;AACA","names":["name","store","data","userinfo","id","type","creator","creatorEmail","score","due","activeName","labelPosition","updateChannel","options","value","label","created","axios","methods","parseTime","date","time","y","m","d","h","i","s","a","handleClick","console","updateChannelNow","message"],"sourceRoot":"src/view","sources":["UpdateChannel.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-button type=\"primary\" @click=\"lookChannel\" style=\"position: absolute;right: 100px;top: 10px;\">获取通道信息\r\n    </el-button>\r\n    <el-tabs v-model=\"activeName\" @tab-click=\"handleClick\" style=\"width: 400px;\" stretch>\r\n      <el-tab-pane label=\"通道信息\" name=\"look\">\r\n        <div>\r\n          通道id：<a>{{ userinfo.id }}</a><br>\r\n          通道名称：<a>{{ userinfo.name }}</a><br>\r\n          通道类型：<a>{{ userinfo.type }}</a><br>\r\n          通道创建者：<a>{{ userinfo.creator }}</a><br>\r\n          通道创建者邮箱：<a>{{ userinfo.creatorEmail }}</a><br>\r\n          通道分数：<a>{{ userinfo.score }}</a><br>\r\n          通道截止日期：<a value-format=\"yyyy-MM-dd HH:mm:ss\">{{ parseTime(userinfo.due) }}</a><br>\r\n        </div>\r\n      </el-tab-pane>\r\n      <el-tab-pane label=\"修改通道信息\" name=\"update\">\r\n        <el-form :label-position=\"labelPosition\" label-width=\"120px\" :model=\"updateChannel\">\r\n          <el-form-item label=\"通道名称\">\r\n            <el-input type=\"string\" v-model=\"userinfo.name\" style=\"width:225px\"\r\n                      placeholder=\"请输入新的通道名称\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"通道类型\">\r\n            <el-select v-model=\"updateChannel.type\" placeholder=\"请选择新的通道类型\">\r\n              <el-option\r\n                  v-for=\"item in options\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"通道创建者\">\r\n            <el-input type=\"string\" v-model=\"userinfo.creator\" style=\"width:225px\"\r\n                      placeholder=\"请输入新的通道创建者\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"通道创建者邮箱\">\r\n            <el-input type=\"email\" v-model=\"userinfo.creatorEmail\" style=\"width:225px\"\r\n                      placeholder=\"请输入新的通道创建者邮箱\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"通道得分\">\r\n            <el-input type=\"string\" v-model=\"userinfo.score\" style=\"width:225px\"\r\n                      placeholder=\"请输入新的通道得分\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"通道截止时间\">\r\n            <el-date-picker\r\n                v-model=\"updateChannel.due\"\r\n                type=\"datetime\"\r\n                placeholder=\"请选择新的通道截止时间\"\r\n                value-format=\"yyyy-MM-dd HH:mm:ss\">\r\n            </el-date-picker>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button type=\"primary\" @click=\"updateChannelNow\">修改通道信息</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-tab-pane>\r\n    </el-tabs>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport store from \"@/vuex/store\";\r\n\r\nexport default {\r\n  name: \"UpdateChannel\",\r\n  store,\r\n  data() {\r\n    return {\r\n      userinfo: {\r\n        id: '',\r\n        name: '',\r\n        type: '',\r\n        creator: '',\r\n        creatorEmail: '',\r\n        score: '',\r\n        due: '',\r\n      },\r\n      activeName: '',\r\n      labelPosition: 'right',\r\n      updateChannel: {\r\n        name: '',\r\n        type: '',\r\n        creator: '',\r\n        creatorEmail: '',\r\n        score: '',\r\n        due: '',\r\n      },\r\n      options: [{\r\n        value: 'Thesis',\r\n        label: '论文'\r\n      }, {\r\n        value: 'Project',\r\n        label: '项目'\r\n      }],\r\n      value: ''\r\n    }\r\n  },\r\n  created: function (){\r\n    this.channelid = this.$store.state.channel.id\r\n    axios.get(`mu/getChannelById?id=${this.channelid}`).then(res => {\r\n      this.userinfo.id = res.data.id\r\n      this.userinfo.name = res.data.name\r\n      if (res.data.type === \"Thesis\") {\r\n        this.userinfo.type = \"论文\"\r\n      } else if (res.data.type === \"Project\") {\r\n        this.userinfo.type = \"项目\"\r\n      }\r\n      this.userinfo.creator = res.data.creator\r\n      this.userinfo.creatorEmail = res.data.creatorEmail\r\n      this.userinfo.score = res.data.score\r\n      this.userinfo.due = res.data.due\r\n    })\r\n  },\r\n  methods: {\r\n    parseTime(time, cFormat) {\r\n      if (arguments.length === 0) {\r\n        return null\r\n      }\r\n      const format = cFormat || '{y}-{m}-{d} {h}:{i}:{s}'\r\n      let date\r\n      if (typeof time === 'object') {\r\n        date = time\r\n      } else {\r\n        if ((typeof time === 'string') && (/^[0-9]+$/.test(time))) {\r\n          time = parseInt(time)\r\n        }\r\n        if ((typeof time === 'number') && (time.toString().length === 10)) {\r\n          time = time * 1000\r\n        }\r\n        date = new Date(time)\r\n      }\r\n      const formatObj = {\r\n        y: date.getFullYear(),\r\n        m: date.getMonth() + 1,\r\n        d: date.getDate(),\r\n        h: date.getHours(),\r\n        i: date.getMinutes(),\r\n        s: date.getSeconds(),\r\n        a: date.getDay()\r\n      }\r\n      const time_str = format.replace(/{([ymdhisa])+}/g, (result, key) => {\r\n        const value = formatObj[key]\r\n        // Note: getDay() returns 0 on Sunday\r\n        if (key === 'a') { return ['日', '一', '二', '三', '四', '五', '六'][value] }\r\n        return value.toString().padStart(2, '0')\r\n      })\r\n      return time_str\r\n    },\r\n    handleClick(tab, event) {\r\n      console.log(tab, event);\r\n    },\r\n    updateChannelNow() {\r\n        this.look = this.$store.state.channel.id\r\n        this.due = this.$data.updateChannel.due.toString()\r\n        axios.post(`mu/updateChannel?id=${this.look}&name=${this.userinfo.name}&type=${this.userinfo.type}&creator=${this.userinfo.creator}&creatorEmail=${this.userinfo.creatorEmail}&score=${this.userinfo.score}&due=${this.$data.updateChannel.due}`).then(res => {\r\n          if (res.status === 400) {\r\n            this.$message({\r\n              message: \"serve error\",\r\n              type: 'warning'\r\n            })\r\n          } else if (res.status === 403) {\r\n            this.$message({\r\n              message: \"Unauthorized\",\r\n              type: 'warning'\r\n            })\r\n          } else if (res.status === 404) {\r\n            this.$message({\r\n              message: \"Name Not Found\",\r\n              type: 'warning'\r\n            })\r\n          } else if (res.status === 200) {\r\n            this.$message({\r\n              message: res.data,\r\n              type: 'success'\r\n            })\r\n          }\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.user {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}