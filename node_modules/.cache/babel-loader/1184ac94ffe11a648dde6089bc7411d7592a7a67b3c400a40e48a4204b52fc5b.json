{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"UserManagement\",\n  data() {\n    return {\n      labelPosition: 'right',\n      ActivationCode: {\n        code: '',\n        name: '',\n        isAdmin: ''\n      }\n    };\n  },\n  methods: {\n    AddActivationCode() {\n      if (this.$data.ActivationCode.code === '') {\n        this.$message({\n          message: '请输入激活码！',\n          type: 'warning'\n        });\n      } else if (this.$data.ActivationCode.name === '') {\n        this.$message({\n          message: '请输入真实姓名！',\n          type: 'warning'\n        });\n      } else if (this.$data.ActivationCode.isAdmin === '') {\n        this.$message({\n          message: '请输入权限！',\n          type: 'warning'\n        });\n      } else {\n        axios.post(`mu/addActivationCode?code=${this.$data.ActivationCode.code}&name=${this.$data.ActivationCode.name}&isAdmin=${this.$data.ActivationCode.isAdmin}`).then(res => {\n          if (res.status === 403) {\n            this.$message({\n              message: ''\n            });\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAsBA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;QACAC;QACAJ;QACAK;MACA;IACA;EACA;EACAC;IACAC;MACA;QACA;UACAC;UACAC;QACA;MACA;QACA;UACAD;UACAC;QACA;MACA;QACA;UACAD;UACAC;QACA;MACA;QACAC;UACA;YACA;cACAF;YACA;UACA;QACA;MACA;IACA;EACA;AACA","names":["name","data","labelPosition","ActivationCode","code","isAdmin","methods","AddActivationCode","message","type","axios"],"sourceRoot":"src/view","sources":["UserManagement.vue"],"sourcesContent":["<template>\r\n  <div>\r\n  <div style=\"width: 300px;\">\r\n  <el-form :label-position=\"labelPosition\" label-width=\"80px\" :model=\"ActivationCode\">\r\n    <el-form-item label=\"激活码\">\r\n      <el-input v-model=\"ActivationCode.code\"></el-input>\r\n    </el-form-item>\r\n    <el-form-item label=\"真实姓名\">\r\n      <el-input v-model=\"ActivationCode.name\"></el-input>\r\n    </el-form-item>\r\n    <el-form-item label=\"权限\">\r\n      <el-input v-model=\"ActivationCode.isAdmin\"></el-input>\r\n    </el-form-item>\r\n    <el-form-item>\r\n      <el-button type=\"primary\" @click=\"AddActivationCode\">添加激活码</el-button>\r\n    </el-form-item>\r\n  </el-form>\r\n  </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"UserManagement\",\r\n  data() {\r\n    return {\r\n      labelPosition: 'right',\r\n      ActivationCode:{\r\n        code:'',\r\n        name:'',\r\n        isAdmin:''\r\n      }\r\n    };\r\n  },\r\n  methods:{\r\n    AddActivationCode(){\r\n      if(this.$data.ActivationCode.code===''){\r\n        this.$message({\r\n          message:'请输入激活码！',\r\n          type:'warning'\r\n        })\r\n      }else if(this.$data.ActivationCode.name===''){\r\n        this.$message({\r\n          message:'请输入真实姓名！',\r\n          type:'warning'\r\n        })\r\n      }else if(this.$data.ActivationCode.isAdmin===''){\r\n        this.$message({\r\n          message:'请输入权限！',\r\n          type:'warning'\r\n        })\r\n      }else{\r\n        axios.post(`mu/addActivationCode?code=${this.$data.ActivationCode.code}&name=${this.$data.ActivationCode.name}&isAdmin=${this.$data.ActivationCode.isAdmin}`).then(res=>{\r\n          if(res.status===403){\r\n            this.$message({\r\n              message:''\r\n            })\r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}